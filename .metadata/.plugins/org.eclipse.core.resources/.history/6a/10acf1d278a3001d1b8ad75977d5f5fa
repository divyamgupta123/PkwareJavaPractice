package com.pkware.practice;


class A{
	void show() {
		System.out.println("A class method called");
	}
}
class B extends A{
	void show() {
		System.out.println("B class method called");
	}
}

public class MethodOverridingProgram {
	public static void main(String[] args) {
		A a = new A();
		B b = new B();
		
		a.show();
		b.show();
	}
}
