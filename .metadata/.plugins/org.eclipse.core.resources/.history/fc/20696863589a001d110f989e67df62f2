package practice;

class BTSNode{
	int data;
	BTSNode left,right;
	BTSNode(int data){
		this.data = data;
		left = null;
		right = null;
	}
}

class BinarySearchTree{
	BTSNode root;
	
	public BinarySearchTree() {
		this.root = null;
	}
	
	BTSNode insertNode(int data){
		if(root == null) {
			return new BTSNode(data);
			
		}
		if(root.data<data) root.right = insertNode(data);
		
	}
}

public class BSTProgram {
	public static void main(String[] args) {
		
	
	}
}
