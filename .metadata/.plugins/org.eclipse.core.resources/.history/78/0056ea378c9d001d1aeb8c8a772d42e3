package com.pkware.accmod;


class DefaultAccClass{
	void displayClass() {
		System.out.println("I am in Default Class");
	}
}

protected class ProtectedAccClass{
	void displayClass() {
		System.out.println("I am in Default Class");
	}
}
private class PrivateAccClass{
	void displayClass() {
		System.out.println("I am in Default Class");
	}
}
public class PublicAccClass{
	void displayClass() {
		System.out.println("I am in Default Class");
	}
}



public class AccessModifiersPractice {

}
