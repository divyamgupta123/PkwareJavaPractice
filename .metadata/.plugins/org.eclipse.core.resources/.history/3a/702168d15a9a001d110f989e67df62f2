package practice;

class BTSNode{
	int data;
	BTSNode left,right;
	BTSNode(int data){
		this.data = data;
		left = null;
		right = null;
	}
}

class BinarySearchTree{
	BTSNode root;
	
	public BinarySearchTree() {
		this.root = null;
	}
	
	BTSNode insertNode(int data){
		if(root == null) {
			root = new BTSNode(data);
			return root;
			
		}
		if(root.data<data) root.right = insertNode(data);
		if(root.data>data) root.left = insertNode(data);
		
		return root;
	}
	
	boolean searchNode(int data) {
		if(root != null) return false;
		if(root.data == data) return true;
		
