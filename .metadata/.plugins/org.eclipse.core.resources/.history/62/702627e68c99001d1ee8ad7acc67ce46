package practice;

class Node{
	int data;
	Node next;
	
	Node(int data){
		this.data = data;
		this.next = null;
	}
}

class LinkedList{
	private Node head;
	
	LinkedList(){
		head = null;
	}
	
	void insertAtBeg(int val) {
		if(head==null) {
			head = new Node(val);
		}
		else {
			Node tmp = head;
			head = new Node(val);
			head.next = tmp;
		}
	}
	
	void insertAtEnd(int val) {
		if(head == null) {
			head = new Node(val);
			return;
		}
		Node tmp = head;
		while(tmp.next!=null) {
			tmp = tmp.next;
		}
		tmp.next = new Node(val);
	}
	
	void insertAtPos(int val,int index){
		if(index>getLength()) {
			System.out.println("Position value is greater than length");
			return;
		}
		if(pos == 0)
	}
	
	int getLength() {
		int len = 0;
		Node tmp = head;
		while(tmp != null) {
			len++;
			tmp = tmp.next;
		}
		return len;
	}
	
}

