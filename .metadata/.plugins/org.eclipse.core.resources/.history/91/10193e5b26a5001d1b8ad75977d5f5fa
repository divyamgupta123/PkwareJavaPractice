package com.pkware.practice;

import java.lang.Cloneable;

class CloneTestClass implements Cloneable{
	String name;
	int rollNo;

	public CloneTestClass(String name, int rollNo) {
		this.name = name;
		this.rollNo = rollNo;
	}
	
	public void display() {
		System.out.println("Roll Number "+this.rollNo+" - "+this.name);
	}

	@Override
	protected Object clone() throws CloneNotSupportedException {
		return super.clone();
	}

}

public class MarkerInterfacesProgram {

	public static void main(String[] args) {
		CloneTestClass cltObj = new CloneTestClass("Divyam", 12);

//		try {
//			CloneTestClass cltObj2 = (CloneTestClass) cltObj.clone();
//		} catch (Exception e) {
//			e.printStackTrace();
//		}
		
		try {
			CloneTestClass cltObj2 = (CloneTestClass) cltObj.clone();
			cltObj2.display();
		} catch (Exception e) {
			e.printStackTrace();
		}
		

	}

}
